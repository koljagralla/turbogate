{
  "spinnerStream": "stdout",
  "progressStream": "stdout",
  "reasonerStream": "stderr",
  "options": [
    {
      "mode": "bundle",
      "project": "tsconfig.json",
      "useSemicolon": true,
      "useBanner": false,
      "useTimestamp": false,
      "quote": "'",
      "directive": "",
      "fileExt": "none",
      "overwrite": true,
      "backup": false,
      "generationStyle": "auto",
      "include": [
        "./src/local/spec/**/*.ts",
        "./src/local/constructs/**/*.ts",
        "./src/local/util/extend-zod-with-open-api.ts",
        "./src/production/**/*.ts"
      ],
      "exclude": [
        "./src/production/index.ts"
      ],
      "skipEmptyDir": true,
      "removeBackup": false,
      "forceYes": false,
      "output": "./src/local"
    }
  ]
}